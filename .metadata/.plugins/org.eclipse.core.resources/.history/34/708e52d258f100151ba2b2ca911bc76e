package cardDeck;

public class Card {
	
	public enum Suits{
		SPADES,
		HEART,
		DIAMOND,
		CLUBS
	}
	
	public enum Ranks{
		ACE,
		TWO,
		THREE,
		FOUR,
		FIVE,
		SIX,
		SEVEN,
		EIGHT,
		NINE,
		TEN,
		JACK,
		QUEEN,
		KING
	}
	
	private Suits suit;
	private Ranks rank;
	
	public Suits getSuit() {
		return suit;
	}
	public void setSuit(Suits suit) {
		this.suit = suit;
	}
	public Ranks getRank() {
		return rank;
	}
	public void setRank(Ranks rank) {
		this.rank = rank;
	}
	
}