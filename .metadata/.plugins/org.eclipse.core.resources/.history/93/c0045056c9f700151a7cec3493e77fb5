package game;

import GUI.StartupWindow;
import cardDeck.CardDeck;

public class Game {
	private CardDeck cardDeck;
	private StartupWindow startupWindow;
	
	public Game(){
		cardDeck = new CardDeck();
		cardDeck.shuffle(30);
		System.out.println(cardDeck.deal());
		
		startupWindow = new StartupWindow();
	}

}
